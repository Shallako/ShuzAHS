      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "value": null, "color": "blue" }, { "value": 10, "color": "green" }] }, "unit": "short" } },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 }
    },
    {
