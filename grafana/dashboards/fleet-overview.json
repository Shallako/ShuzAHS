{
  "title": "Komatsu AHS - Fleet Overview",
  "tags": ["ahs", "fleet", "komatsu", "cep", "alerts"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 3,
  "refresh": "5s",
  "uid": "ahs-fleet-overview",
  "panels": [
    {
      "id": 1, "title": "Total Fleet Size", "type": "stat",
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [{ "expr": "ahs_fleet_vehicles_total", "refId": "A" }],
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "value": null, "color": "green" }] }, "unit": "short" } },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 }
    },
    {
      "id": 2, "title": "Active Vehicles", "type": "stat",
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
